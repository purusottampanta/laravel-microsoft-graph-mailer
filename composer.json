{
    "name": "purusottampanta/laravel-microsoft-graph-mailer",
    "description": "Laravel mail transport for Microsoft Graph API",
    "homepage": "https://github.com/purusottampanta/laravel-microsoft-graph-mailer",
    "keywords": ["laravel", "mailer", "microsoft", "graph", "api", "mail"],
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Purusottam Panta",
        "email": "purusottampanta@gmail.com"
      }
    ],
    "require": {
      "php": "^8.1",
      "illuminate/mail": "^9.21|^10.0|^11.0|^12.0",
      "illuminate/support": "^9.21|^10.0|^11.0|^12.0",
      "symfony/mailer": "^6.0",
      "symfony/mime": "^6.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0",
      "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
      "psr-4": {
        "Puru\\GraphMailer\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Puru\\GraphMailer\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Puru\\GraphMailer\\MicrosoftGraphMailServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    }
    
}
